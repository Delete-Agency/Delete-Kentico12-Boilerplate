<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions parameters="ConsentID">
    <action name="edit" caption="$General.Edit$" fonticonclass="icon-edit" fonticonstyle="allow" />
    <action name="#delete" caption="$General.Delete$" fonticonclass="icon-bin" fonticonstyle="critical" confirmation="$General.ConfirmDelete$" />
  </actions>
  <columns>
    <column source="ConsentDisplayName" caption="$dataprotection.consents.grid.column.consentdisplayname$" wrap="false" localize="true">
      <Filter Type="text" />
    </column>
    <column source="ConsentLastModified" caption="$dataprotection.consents.grid.column.lastmodified$" wrap="false" localize="false" >
    </column>
    <column cssclass="filling-column" />
  </columns>
  <objecttype name="cms.consent" columns="ConsentID,ConsentDisplayName,ConsentLastModified" />
  <options>
    <key name="DisplayFilter" value="true" />
  </options>
</grid>
