<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions parameters="AccountStatusID">
    <action name="edit" caption="$General.Edit$" fonticonclass="icon-edit" fonticonstyle="allow" />
    <action name="#delete" caption="$General.Delete$" fonticonclass="icon-bin" fonticonstyle="critical" confirmation="$general.deleteconfirmation$" />
  </actions>
  <columns>
    <column source="AccountStatusDisplayName" caption="$om.accountstatus.displayname$" wrap="false" localize="true" >
      <filter type="text" size="200" />
    </column>
    <column cssclass="filling-column" />
  </columns>
  <objecttype name="om.accountstatus" columns="AccountStatusID,AccountStatusDisplayName" />
  <options>
    <key name="DisplayFilter" value="true" />
  </options>
</grid>
