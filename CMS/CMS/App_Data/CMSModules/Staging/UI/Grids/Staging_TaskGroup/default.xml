<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions parameters="TaskGroupID">
    <action name="edit" caption="$General.Edit$" fonticonclass="icon-edit" fonticonstyle="allow" />
    <action name="#delete" caption="$General.Delete$" fonticonclass="icon-bin" fonticonstyle="critical" confirmation="$General.ConfirmDelete$" />
  </actions>
  <columns>
    <column source="TaskGroupCodeName" caption="$general.codename$" wrap="false" maxlength="100" localize="true" cssclass="main-column-100"/>
    <column cssclass="filling-column" />
  </columns>
  <objecttype name="staging.taskgroup" columns="TaskGroupID, TaskGroupCodeName" />
  <options>
    <key name="DisplayFilter" value="false" />
  </options>
</grid>
