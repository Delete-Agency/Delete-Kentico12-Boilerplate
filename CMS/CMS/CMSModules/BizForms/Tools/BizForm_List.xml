<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions parameters="FormID;FormSiteID">
    <action name="edit" caption="$General.Edit$" fonticonclass="icon-edit" fonticonstyle="allow" />
    <action name="delete" caption="$General.Delete$" fonticonclass="icon-bin" fonticonstyle="critical" confirmation="$General.ConfirmDelete$" />
  </actions>
  <columns>
    <column source="FormDisplayName" caption="$BizForm.Columns.FormName$" wrap="false" localize="true">
      <filter type="text" />
    </column>
    <column source="FormItems" caption="$BizForm.Columns.FormEntries$" wrap="false">
    </column>
    <column cssclass="main-column-100">
    </column>
  </columns>
  <objecttype name="cms.form" columns="FormID, FormAccess, FormSiteID, FormDisplayName, FormItems" />
  <options>
    <key name="DisplayFilter" value="true" />
  </options>
</grid>
