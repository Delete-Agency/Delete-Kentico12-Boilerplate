<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions parameters="ROUTINE_NAME;IsCustom">
    <action name="edit" commandargument="ROUTINE_NAME" caption="$General.Edit$" fonticonclass="icon-edit" fonticonstyle="allow" />
    <action name="delete" externalsourcename="delete" commandargument="{0};{1}" caption="$General.Delete$" fonticonclass="icon-bin" fonticonstyle="critical" confirmation="$general.confirmdelete$"/>
	</actions>
	<columns>
    <column source="ROUTINE_SCHEMA" caption="$systbl.proc.schema$" wrap="false">
    </column>
	<column source="ROUTINE_NAME" caption="$general.name$" wrap="false" cssclass="main-column-100">
	</column>
    <column source="IsCustom" externalsourcename="iscustom" caption="$systbl.proc.iscustom$" wrap="false">
    </column>
  </columns>
	<options>
		<key name="DisplayFilter" value="false" />
	</options>
</grid>
